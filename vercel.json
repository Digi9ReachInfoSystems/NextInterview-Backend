{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
   "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }
    ],
    "crons": [
    {
      "path": "/cron/removeRestriction",
      "schedule": "0 0 * * *"
    }
  ]
  }