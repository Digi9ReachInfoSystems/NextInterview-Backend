{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
   "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "headers": [
                {
                  "key": "access-control-allow-origin",
                  "value": "*"
                }, 
                {
                  "key": "Access-Control-Allow-Methods",
                  "value": "GET,HEAD,PUT,PATCH,POST,DELETE" 
                }
            ]
      }
    ],
    "crons": [
    {
      "path": "/cron/removeRestriction",
      "schedule": "0 0 * * *"
    }
  ]
  }